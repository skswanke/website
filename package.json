{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "date-fns": "^2.29.3",
    "exif": "^0.6.0",
    "gray-matter": "^4.0.3",
    "next": "12.1.4",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/exif": "^0.6.3",
    "@types/node": "^18.6.5",
    "@types/react": "^18.0.1",
    "@types/styled-components": "^5.1.25",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.4",
    "prettier": "^2.7.1"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": false
            }
          ]
        ]
      }
    }
  }
}
